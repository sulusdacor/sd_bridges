    <pathCost>0</pathCost>
    <acceptFilth>false</acceptFilth>
    <fertility>0</fertility>
    <avoidWander>true</avoidWander>
    <changeable>true</changeable>    <!-- changed the changeable to true -->
		<holdSnow>false</holdSnow>
    <driesTo>Gravel</driesTo>
	<affordances>
      <li>Undefined</li>  <!-- added affordance undefined, needed for building with this mod-->
    </affordances>
	<takeSplashes>true</takeSplashes>
    <tags>
      <li>Water</li>
    </tags>
  </TerrainDef>
  
   <!--========================= mud =============================-->
  
  <TerrainDef>
    <defName>sd_bridges_fakeMud</defName>
    <label>fake mud</label>
    <texturePath>Terrain/Surfaces/Mud</texturePath>
    <edgeType>FadeRough</edgeType>
    <renderPrecedence>330</renderPrecedence>
	<passability>PassThroughOnly</passability>
    <pathCost>0</pathCost>
    <statBases>
      <Beauty>-2</Beauty>
      <Cleanliness>-2</Cleanliness>
    </statBases>
    <terrainFilthDef>FilthDirt</terrainFilthDef>
    <fertility>0</fertility>
    <takeFootprints>True</takeFootprints>
    <acceptFilth>false</acceptFilth>
    <avoidWander>true</avoidWander>
    <driesTo>Soil</driesTo>
	<affordances>
      <li>Undefined</li> <!-- added affordance undefined, needed for building with this modwith this mod-->
	  <li>Diggable</li>  <!-- added diggable here for vegetable garden mod. need this that you can place the soil over here with this -->
    </affordances>

  </TerrainDef>
  
